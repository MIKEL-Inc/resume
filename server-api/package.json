{
  "name": "server-api",
  "version": "0.0.0",
  "description": "GraphQL API endpoint and static Angular app server",
  "main": "server.js",
  "scripts": {
    "predev": "npm run build",
    "dev": "concurrently -p \"[{name}]\" -n \"TypeScript,ExpressWeb\" -c \"bgBlue.dim.italic,bgGreen.dim.italic\" \"npm run watch\" \"npm run serve\" ",
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w",
    "serve": "PGUSER=postgres PGHOST=localhost PGPASSWORD=postgres PGDATABASE=postgres PGPORT=5432 NODE_ENV=dev nodemon dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "graphql",
    "api",
    "resume",
    "angular",
    "static",
    "http-server"
  ],
  "author": "MIKEL, Inc.",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "express-promise-router": "^3.0.3",
    "graphql": "^14.0.2",
    "pg": "^7.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/express-graphql": "^0.6.2",
    "@types/express-promise-router": "^2.0.0",
    "@types/graphql": "^14.0.3",
    "@types/pg": "^7.4.11",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.8",
    "typescript": "^3.2.2"
  }
}
